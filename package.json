{
  "name": "sunbird-quml-player",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build quml-library --watch=true",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "create-player-link": "cd dist/quml-library && npm link && cd ../..",
    "link-player": "cd dist/quml-library && npm link && cd ../.. && npm link @project-sunbird/sunbird-quml-player-v9",
    "build-lib": "ng build quml-library && node scripts/copyAssets.js",
    "build-lib-link": "npm run build-lib && npm run link-player",
    "test-lib": "ng test quml-library",
    "test-lib-coverage": "ng test quml-library --code-coverage=true",
    "test-lib-ci": "ng test quml-library --watch=false --code-coverage --sourceMap=false",
    "mybuild": "npm run build",
    "serve": "node scripts/copyAssets.js && npm link ./dist/quml-library && ng serve --vendor-source-map",
    "build-web-component": "npm run build-lib-link && ng build quml-player-wc --output-hashing none && node ./build-wc.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.12",
    "@angular/common": "^9.1.12",
    "@angular/compiler": "^9.1.12",
    "@angular/core": "^9.1.12",
    "@angular/elements": "^12.2.4",
    "@angular/forms": "^9.1.12",
    "@angular/platform-browser": "^9.1.12",
    "@angular/platform-browser-dynamic": "^9.1.12",
    "@angular/router": "^9.1.12",
    "@project-sunbird/client-services": "4.0.1",
    "@project-sunbird/sb-styles": "0.0.9",
    "@project-sunbird/telemetry-sdk": "0.0.26",
    "bootstrap": "4.1.1",
    "core-js": "^2.5.4",
    "document-register-element": "^1.7.2",
    "jquery": "^3.5.1",
    "katex": "^0.11.1",
    "lodash-es": "^4.17.21",
    "ngx-bootstrap": "^5.6.2",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "uuid": "^3.4.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.15",
    "@angular-devkit/build-ng-packagr": "~0.901.15",
    "@angular/cli": "^9.1.12",
    "@angular/compiler-cli": "^9.1.12",
    "@angular/language-service": "^9.1.12",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "concat": "^1.0.3",
    "fs-extra": "^8.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.1",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.7.3"
  }
}
